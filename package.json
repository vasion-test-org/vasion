  "private": true,
  "scripts": {
    "clean": "rimraf .next",
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "checks": "node scripts/checks/index.mjs",
    "checks:ada": "node scripts/checks/ada.mjs",
    "checks:seo": "node scripts/checks/seo.mjs",
    "checks:perf": "node scripts/checks/performance.mjs",
    "proxy": "mkcert -install && mkcert localhost && local-ssl-proxy --source 3010 --target 3000 --cert localhost.pem --key localhost-key.pem",
